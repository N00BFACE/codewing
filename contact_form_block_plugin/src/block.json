{
    "apiVersion": 2,
    "name": "create-block/contact-form",
    "version": "0.1.0",
    "title": "Contact Form",
    "category": "widgets",
    "icon": "email-alt",
    "description": "Custom block for contact form plugin.",
    "supports": {
        "html": false
    },
    "attributes": {
        "content": {
            "type": "array",
            "source": "children",
            "selector": "p"
        },
        "title": {
            "type": "string",
            "selector": "h2"
        },
        "selectedPost": {
            "type": "object",
            "post_tye": "metablocks",
            "default": 0
        },
        "postid": {
            "type": "string"
        }
    },
    "textdomain": "contact-form",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./editor.css"
}